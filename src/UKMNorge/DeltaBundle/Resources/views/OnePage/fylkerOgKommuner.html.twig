                    for(kommune of fylke.kommuner) {
                        
                        html += `
                        <div id="searchKommuner" class="kommune-accordion collapsed" k_id="${kommune.id}" data-toggle="collapse" data-parent="#accordionKommune" href="#collapseArrangementer${kommune.id}">
