{% extends "UKMDeltaBundle::layout.html.twig" %}

{% block content %}
	<h1 class="center">Hva vil du melde p√•?</h1>
	
	<p>&nbsp;</p>
	{# <p class="lead center">
		Velg hvilken type innslag du vil delta med.
	</p> #}
	<div>
		{% for type in typer %} 
			{% if loop.first %}
				<p class="lead center">Vise frem noe</p>
			{% elseif loop.last %}
				<p class="lead center">Jobbe med UKM</p> 
			{% endif %}

			<ul class="list list-unstyled list-group">
			{% for enkelttype in type %}
				<li>
					<div class="row">
						<div class="col-md-9 col-xs-9">
							<a href="{{ path('ukm_delta_ukmid_pamelding_' ~ enkelttype.ico ~ '_hvem', 
							{ 	k_id: k_id,  
								pl_id: pl_id }) }}">{{ enkelttype.name|raw }}</a> 
						</div>
						<div class="col-md-3 col-xs-3">
							Ikon her
						</div>
					</div>
				</li>
			{% endfor %}
			</ul> 
		{% endfor %}
	</div>
{% endblock %}
