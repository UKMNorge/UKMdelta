{% extends "UKMDesignBundle::layout.html.twig" %}

{% block css %}
	{{ parent() }}
	{% stylesheets '@UKMDeltaBundle/Resources/public/less/*' filter='less' output='css/compiled-main.css' %}
	    <link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
{% endblock %}

{% block content_pre %}
	{% for type, messages in app.session.flashBag.all %}
		{% for message in messages %}	
			{{ message|trans }}		
		{% endfor %}
	{% endfor %}
{% endblock %}