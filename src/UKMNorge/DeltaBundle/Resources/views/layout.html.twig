{% extends 'UKMDesign/Layout/base.html.twig'|UKMpath %}

{% block javascripts %}
	{% block additional_js %}{% endblock %}
    {% javascripts 
        '@UKMDeltaBundle/Resources/public/js/ukmdesignbundle.js'
        '@UKMDeltaBundle/Resources/public/js/h5validate.jquery.js'
        '@UKMDeltaBundle/Resources/public/js/autogrow.jquery.js'
        '@UKMDeltaBundle/Resources/public/js/general.js'
        'https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js'
        '%kernel.root_dir%/../vendor/ukmnorge/spa/eventElement.js'
        '%kernel.root_dir%/../vendor/ukmnorge/spa/UKMOnePage.js'
        '@UKMDeltaBundle/Resources/public/js/deltaOnePage.js'
        '%kernel.root_dir%/../vendor/ukmnorge/spa/director.js'
        '%kernel.root_dir%/../vendor/ukmnorge/spa/SPAInteraction.js'
         %}
        <script src="{{ asset_url }}"></script> 
    {% endjavascripts %}
{% endblock %}

{% block content %}

	{% stylesheets
		'@UKMDeltaBundle/Resources/public/css/index.css' %}
		<link type="text/css" rel="stylesheet" media="all" href="{{ asset_url }}" />
	{% endstylesheets %}
	
	{% if app.session.flashBag.peekAll()|length > 0 %}
		<div class="container">
			<div class="row">
				<div class="col-12">
					{% embed "UKMDeltaBundle:FlashBag:list.html.twig" %}{% endembed %}
				</div>
			</div>
		</div>
	{% endif %}

	{% block delta_content %}Designfil mangler delta_content-block.{% endblock %}
{% endblock %}

